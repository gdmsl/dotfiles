# This tmux statusbar config was created by tmuxline.vim
# on Wed, 09 Dec 2015

set -g status-bg "#393939"
set -g message-command-fg "#a09f93"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "#515151"
set -g message-bg "#515151"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "#a09f93"
set -g message-command-bg "#515151"
set -g status-attr "none"
set -g status-utf8 "on"
set -g pane-border-fg "#393939"
set -g status-left-attr "none"
setw -g window-status-fg "#747369"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "#393939"
setw -g window-status-activity-attr "underscore"
setw -g window-status-activity-fg "#747369"
setw -g window-status-separator ""
setw -g window-status-bg "#393939"
set -g status-left "#[fg=#747369,bg=#393939] #S #[fg=#393939,bg=#393939,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#393939,bg=#393939,nobold,nounderscore,noitalics]#[fg=#747369,bg=#393939] %R "
setw -g window-status-format "#[fg=#393939,bg=#393939,nobold,nounderscore,noitalics]#[default] #I #W #[fg=#393939,bg=#393939,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=#393939,bg=#515151,nobold,nounderscore,noitalics]#[fg=#a09f93,bg=#515151] #I #W #[fg=#515151,bg=#393939,nobold,nounderscore,noitalics]"
