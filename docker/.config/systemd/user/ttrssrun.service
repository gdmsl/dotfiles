[Unit]
Description=TTRSS
Requires=/usr/lib/systemd/system/docker.service ttrssdb.service
After=/usr/lib/systemd/system/docker.service ttrssdb.service

[Service]
Restart=always
ExecStart=/usr/bin/docker start -a ttrssrun
ExecStop=/usr/bin/docker stop -t 2 ttrssrun

[Install]
WantedBy=multi-user.target
